@using CarInsurance.ViewModels
@model List<AdminVm>
@{
    ViewBag.Title = "Admin View";
}

<h2>Admin View</h2>

<table>
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Quote</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var insuree in Model)
            {
                <tr>
                    <td>@insuree.FirstName</td>
                    <td>@insuree.LastName</td>
                    <td>@insuree.Quote</td>
                </tr>
            }

    </tbody>
</table>

